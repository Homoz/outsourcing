CREATE TABLE staff_info(
id integer primary key, 
name text not null,
gender text not null,
type int not null,
department int not null,
birthDay text not null,
title text not null,
education text not null,
salary double not null,
isMarried tinyint not null);


insert into staff_info(name, gender, type, department, birthDay, title, education, salary, isMarried)
values( "张三", "男", 1,0, "1957-10-12 00:00:00.000", "院长", "高中", 9527.2, 1);
